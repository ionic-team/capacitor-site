{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/3",
      "destination": "https://capacitorjs.com/blog/announcing-capacitor-3-0-rc"
    },
    {
      "source": "/forum",
      "destination": "https://forum.ionicframework.com/c/capacitor/26"
    },
    {
      "source": "/docs/reference/cli",
      "destination": "/docs/cli"
    },
    {
      "source": "/docs/basics/cli-commands",
      "destination": "/docs/cli"
    },
    {
      "source": "/docs/community/plugins",
      "destination": "/docs/plugins/community"
    },
    {
      "source": "/docs/community",
      "destination": "/docs/plugins/community"
    },
    {
      "source": "/docs/pwa-elements",
      "destination": "/docs/web/pwa-elements"
    },
    {
      "source": "/docs/v3/getting-started/dependencies",
      "destination": "/docs/v3/getting-started/environment-setup"
    },
    {
      "source": "/docs/v3/basics/opening-native-projects",
      "destination": "/docs/v3/basics/workflow"
    },
    {
      "source": "/docs/v3/basics/running-your-app",
      "destination": "/docs/v3/basics/workflow"
    },
    {
      "source": "/docs/v3/cordova/using-cordova-plugins",
      "destination": "/docs/v3/plugins/cordova"
    },
    {
      "source": "/docs/v3/cordova/known-incompatible-plugins",
      "destination": "/docs/v3/plugins/cordova"
    },
    {
      "source": "/docs/v3/apis/accessibility",
      "destination": "/docs/v3/apis/screen-reader"
    },
    {
      "source": "/docs/v3/basics/utilities",
      "destination": "/docs/v3/core-apis/web"
    },
    {
      "source": "/docs/v3/ios/updating",
      "destination": "/docs/v3/updating/apps"
    },
    {
      "source": "/docs/v3/android/updating",
      "destination": "/docs/v3/updating/apps"
    },
    {
      "source": "/docs/v3/plugins/js",
      "destination": "/docs/v3/plugins/web"
    },
    {
      "source": "/docs/v3/basics/building-your-app",
      "destination": "/docs/v3/basics/workflow"
    },
    {
      "source": "/docs/v3/basics/progressive-web-app",
      "destination": "/docs/v3/web/progressive-web-apps"
    },
    {
      "source": "/docs/v3/(.*)",
      "destination": "/docs/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)/page.state.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
