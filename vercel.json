{
  "trailingSlash": false,
  "rewrites": [
    { "source": "/docs", "destination": "https://next.capacitorjs.com/docs" },
    { "source": "/docs/:match*", "destination": "https://next.capacitorjs.com/docs/:match*" }
  ],
  "redirects": [
    {
      "source": "/3",
      "destination": "https://ionicframework.com/blog/announcing-capacitor-3-0/"
    },
    {
      "source": "/forum",
      "destination": "https://forum.ionicframework.com/c/capacitor/26"
    }
  ],
  "headers": [
    {
      "source": "/(.*)/page.state.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
